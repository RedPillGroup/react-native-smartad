CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/Smart-Display-SDK
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Smart-Core-SDK" "${PODS_ROOT}/Smart-Display-SDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
OTHER_LDFLAGS = $(inherited) -framework "AVFoundation" -framework "Accelerate" -framework "AdSupport" -framework "CoreGraphics" -framework "CoreLocation" -framework "CoreMedia" -framework "CoreMotion" -framework "Foundation" -framework "MediaPlayer" -framework "QuartzCore" -framework "SCSCoreKit" -framework "StoreKit" -framework "SystemConfiguration" -framework "UIKit" -weak_framework "SafariServices" -weak_framework "SceneKit" -weak_framework "SpriteKit" -weak_framework "WebKit"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/Smart-Display-SDK
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
